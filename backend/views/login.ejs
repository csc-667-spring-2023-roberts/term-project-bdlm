<html>
<!--  -->

<head>
    <title>
        <%= title %>
    </title>
    <script src="/scripts/bundle.js"></script>
    <link rel="stylesheet" href="/stylesheets/home.css" />
</head>

<body>
    <h1> Login Page (unauthenticated)</h1>
    <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/authentication/register">Sign Up</a></li>
    </ul>
    <form method="post" action="/authentication/login">
        <label>
          <em>Username</em>
          <input name="username" autocomplete="username" placeholder="Username" />
        </label>
        <label>
          <em>Password</em>
          <input name="password" type="password" autocomplete="current-password"
          />
        </label>
        <input type="submit" value="Login" />
      </form>
</body>

</html>